{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "noEmit": true,
    "incremental": true,
    "module": "esnext",
    "esModuleInterop": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@/*": ["src/*"]
    },
    "plugins": [
      {
        "name": "next"
      }
    ]
  },
"include": [
  "next-env.d.ts",
  ".next/types/**/*.ts",
  "src/**/*.ts",
  "src/**/*.tsx",
  "src/app/api/uploads/route",
  "src/database/db.js",
  "src/models/staff.js",
  "src/models/Student.js",
  "src/app/Staff/(StaffPage)/layout.jsx"
],
  "exclude": [
    "node_modules"
  ]
}